<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DrawingView">
        <attr name="brush_color" format="color"/>
        <attr name="drawing_background_color" format="color"/>
        <attr name="brush_size" format="float" />
        <attr name="brush" format="integer">
            <enum name="pen" value="0"/>
            <enum name="pencil" value="1"/>
            <enum name="calligraphy" value="2"/>
            <enum name="air_brush" value="3"/>
            <enum name="eraser" value="4"/>
        </attr>
    </declare-styleable>
</resources>